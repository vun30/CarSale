{
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.hyundaigialaiofficial.com.vn" }],
      "destination": "https://hyundaigialaiofficial.com.vn/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "header", "key": "x-forwarded-proto", "value": "http" }
      ],
      "destination": "https://hyundaigialaiofficial.com.vn/:path*",
      "permanent": true
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/" }]
}
